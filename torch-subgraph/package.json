{
  "name": "torch-subgraph",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "compile": "mustache config/testnet.json subgraph.template.yaml > subgraph.yaml && graph codegen && graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ torch-prediction-market",
    "create-local": "graph create --node http://localhost:8020/ TorchPredictionMarket",
    "remove-local": "graph remove --node http://localhost:8020/ TorchPredictionMarket",
    "deploy-local": "graph deploy --node https://subgraph.xpeerience.io/ --ipfs http://localhost:5001 TorchPredictionMarket",
    "test": "graph test",
    "graph-node": "docker-compose -f ./graph-node/docker-compose.yaml up -d",
    "graph-local-clean": "docker-compose down -v && docker-compose rm -v && rm -rf data/ipfs data/postgres",
    "graph-codegen": "graph egcoden"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.97.1",
    "@graphprotocol/graph-ts": "^0.38.1"
  },
  "devDependencies": {
    "matchstick-as": "^0.6.0"
  }
}
